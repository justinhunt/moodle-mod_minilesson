<div
        data-video="{{itemytvideoid}}"
        data-start="{{itemytvideostart}}"
        data-end="{{itemytvideoend}}"
        data-height="560"
        data-width="315"
        id="{{uniqid}}_ytplayer"
></div>
{{^element.frozen}}
    {{#js}}
        require(['jquery', 'mod_minilesson/youtubeclip'], function($, youtubeclip) {
            youtubeclip.initVideo("{{uniqid}}_ytplayer");
        });
    {{/js}}
{{/element.frozen}}